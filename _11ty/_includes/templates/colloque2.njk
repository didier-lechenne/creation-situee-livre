
<!-- _11ty/_layouts/colloque2.njk -->
<section id="{{ p.fileSlug | cleanTitle | slug }}" 
         data-template="colloque2"
         style="{% if p.data.bgColor %}--bg-color: {{ p.data.bgColor }}; {% endif %}"
         {% if p.data.bgColor %}data-bg-color="{{ p.data.bgColor }}" {% endif %}
         {% if p.data.show %}data-show="{{ p.data.show }}"{% endif %}
         {% if p.data.toc %}data-toc="{{ p.data.toc }}"{% endif %}
         {% if p.data.moveTo %}data-page-move-to="{{ p.data.moveTo }}" {% endif %}
         class="{% if p.data.toc %}toc-{{ p.data.toc }}{% endif %} {% if p.data.class %}{{ p.data.class }}{% endif %}"
	{% include "partials/frontmatter-attrs.njk" %}
  >


  {{ p.templateContent | safe  }} 

</section>